}
<div class="container">
    <div class="jumbotron">
        <h2>BookCave &#64; DATAdump</h2>
    </div>
    <ul>
        @foreach(var book in @Model)
        {
            <li>Id: @book.Id ; Title: @book.Title</li>
        }
    </ul>
</div>